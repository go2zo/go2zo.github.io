{
  "name": "gridsome-go2zo-blog",
  "private": true,
  "scripts": {
    "build": "gridsome build",
    "develop": "gridsome develop",
    "explore": "gridsome explore",
    "lint": "eslint --fix",
    "deploy": "gridsome build && gh-pages -d dist"
  },
  "dependencies": {
    "@gridsome/plugin-google-analytics": "^0.1.0",
    "@gridsome/remark-prismjs": "^0.5.0",
    "@gridsome/source-filesystem": "^0.6.0",
    "@gridsome/transformer-remark": "^0.6.0",
    "@noxify/gridsome-plugin-remark-embed": "^1.3.0",
    "bulma": "^0.8.0",
    "gridsome": "^0.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-gridsome": "^1.2.5",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-vue": "^7.0.0",
    "gh-pages": "^3.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "./scripts/add_issue_number_into_commit.sh $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "yarn lint",
      "git add"
    ]
  }
}
